/*
function maskClouds(image) {
  // Bits 3 and 5 are cloud shadow and cloud, respectively.
  var cloudShadowBitMask = 1 << 3;
  var cloudsBitMask = 1 << 5;
  // Get the pixel QA band.
  var qa = image.select('QA_PIXEL');
  // Both flags should be set to zero, indicating clear conditions.
  var mask = qa.bitwiseAnd(cloudShadowBitMask).eq(0)
      .and(qa.bitwiseAnd(cloudsBitMask).eq(0));
  // Return the masked image, scaled to reflectance, without the QA bands.
  return image.updateMask(mask).divide(10000)
      //.select("SR_B[0-9]*")
      .copyProperties(image, ["system:time_start"]);
}
*/
function maskClouds(col) {
  // Bits 3 and 5 are cloud shadow and cloud, respectively.
  var cloudShadowBitMask = (1 << 3);
  var cloudsBitMask = (1 << 5);
  // Get the pixel QA band.
  var qa = col.select('QA_PIXEL');
  // Both flags should be set to zero, indicating clear conditions.
  var mask = qa.bitwiseAnd(cloudShadowBitMask).eq(0)
               .and(qa.bitwiseAnd(cloudsBitMask).eq(0));
  return col.updateMask(mask);
}

// Applies scaling factors.
function applyScaleFactors(image) {
  var opticalBands = image.select('SR_B.').multiply(0.0000275).add(-0.2);
  var thermalBands = image.select('ST_B.*').multiply(0.00341802).add(149.0);
  return image.addBands(opticalBands, null, true)
              .addBands(thermalBands, null, true);
}

// adding spectral indices to bands
var indices = function(img){
  var ndvi = img.normalizedDifference(['SR_B5', 'SR_B4']).rename('NDVI');
  var ndbi = img.normalizedDifference(['SR_B6', 'SR_B5']).rename('NDBI');
  var gndvi = img.normalizedDifference(['SR_B6', 'SR_B3']).rename('GNDVI');
  
  return img.addBands(ndvi).addBands(ndbi).addBands(gndvi);
};

var indices_2000 = function(img){
  var ndvi = img.normalizedDifference(['SR_B4', 'SR_B3']).rename('NDVI');
  var ndbi = img.normalizedDifference(['SR_B5', 'SR_B4']).rename('NDBI');
  var gndvi = img.normalizedDifference(['SR_B5', 'SR_B2']).rename('GNDVI');
  
  return img.addBands(ndvi).addBands(ndbi).addBands(gndvi);
};
// adding elevation and slope to bands
var elevation = function(img){
  var elevation = dem.select('elevation');
  var slope = ee.Terrain.slope(elevation);
  
  return img.addBands(elevation).addBands(slope);
};

var l8 = ee.ImageCollection("LANDSAT/LC08/C02/T1_L2") 
         .filterDate("2020-01-01", "2020-12-30")
         .filterMetadata("CLOUD_COVER", "less_than", 30)
         .map(applyScaleFactors)
         .map(maskClouds)
         .map(indices)
         .map(elevation)
         .median()
         .clip(roi);
         
         
var l7_2000 = ee.ImageCollection("LANDSAT/LE07/C02/T1_L2") 
         .filterDate("2000-01-01", "2000-12-30")
         .filterMetadata("CLOUD_COVER", "less_than", 30)
         .map(applyScaleFactors)
         .map(maskClouds)
         .map(indices_2000)
         .map(elevation)
         .median()
         .clip(roi);

var l8_2 = ee.ImageCollection("LANDSAT/LC08/C02/T1_L2") 
         .filterDate("2020-01-01", "2020-12-30")
         .filterMetadata("CLOUD_COVER", "less_than", 30)
         .map(applyScaleFactors)
         .map(maskClouds)
         .map(indices)
         .map(elevation)
         .median()
         .clip(pnc);

  
print(l8);

Map.addLayer(l8, trueColVis, 'Natural colour roi1 2020');
Map.addLayer(l7_2000, trueColVis, 'Natural colour roi1 2010');

Map.addLayer(l8, falseCol543Vis, 'False color (543)');

Map.addLayer(l8_2, trueColVis2, 'Natural color roi2');

Map.addLayer(l8.select('NDVI'), ndviVisParam, 'NDVI');
Map.addLayer(l8_2.select('NDVI'), ndviVisParam, 'NDVI roi2');

Map.addLayer(l8.select('NDBI'), ndbiVisParam, 'NDBI');


// Visualization parameters for elevation
var elevationVis = {
  min: 99.04,
  max: 148.96,
  palette: ['blue', 'yellow', 'red']
};

// Visualization parameters for slope
var slopeVis = {
  min: 0,
  max: 60,
};

// Display the Landsat image with elevation and slope
Map.addLayer(l8.select('elevation'), elevationVis, 'Elevation');
Map.addLayer(l8.select('slope'), slopeVis, 'Slope');

var dataset = ee.ImageCollection('ESA/WorldCover/v200').first();

var visualization = {
  bands: ['Map'],
};

Map.centerObject(dataset);

Map.addLayer(dataset.clip(pnc), visualization, 'Landcover nps');
             
var training = urban.merge(water).merge(vegetation).merge(agricolture);
var sample = l8.sampleRegions({
  collection: training,
  properties: ['Class'],
  scale: 30
})

print(sample);
print('Samples size:', sample.size());
// The randomColumn() method will add a column of uniform random
// numbers in a column named 'random' by default.
sample = sample.randomColumn();

var split = 0.7;  // Roughly 70% training, 30% testing.
var trainSet = sample.filter(ee.Filter.lt('random', split));
print('Training size:', trainSet.size());
var validationSet = sample.filter(ee.Filter.gte('random', split));
print('Validation size:', validationSet.size());

//bands (features) used for training the model
var trainingBands = ['SR_B2', 'SR_B3', 'SR_B4', 'SR_B5', 'SR_B6', 'SR_B7', 'NDVI', 'NDBI', 'GNDVI', 'elevation', 'slope']
//print(l8.bandNames().slice(1));
var classifier = ee.Classifier.smileRandomForest(100).train({
  features: trainSet,
  classProperty: 'Class',
  inputProperties: trainingBands
});

var classified = l8.classify(classifier);

//----------------------- ACCURACY ASSESMENT ----------------------------------

// Get a confusion matrix representing resubstitution accuracy.
var trainAccuracy = classifier.confusionMatrix();
print('Resubstitution error matrix: ', trainAccuracy);
print('Training overall accuracy: ', trainAccuracy.accuracy());
print('Training producers accuracy:', trainAccuracy.producersAccuracy());
print('Training consumers accuracy:', trainAccuracy.consumersAccuracy());
print('Training kappa', trainAccuracy.kappa());

// Classify the validation data.
var validated = validationSet.classify(classifier);

// Get a confusion matrix representing expected accuracy.
var testAccuracy = validated.errorMatrix('Class', 'classification');
print('Validation error matrix: ', testAccuracy);
print('Validation overall accuracy: ', testAccuracy.accuracy());
print('Validation producers accuracy:', testAccuracy.producersAccuracy());
print('Validation consumers accuracy:', testAccuracy.consumersAccuracy());
print('Validation kappa', testAccuracy.kappa());


//------------------------- TRAINING 2000 ---------------------------------------

var sample = l7_2000.sampleRegions({
  collection: training,
  properties: ['Class'],
  scale: 30
})

//print(sample);
//print('Samples size:', sample.size());
// The randomColumn() method will add a column of uniform random
// numbers in a column named 'random' by default.
sample = sample.randomColumn();

var split = 0.7;  // Roughly 70% training, 30% testing.
var trainSet = sample.filter(ee.Filter.lt('random', split));
var validationSet = sample.filter(ee.Filter.gte('random', split));

//bands (features) used for training the model
var trainingBands = ['SR_B1', 'SR_B2', 'SR_B3', 'SR_B4', 'SR_B5', 'SR_B7', 'NDVI', 'NDBI', 'GNDVI', 'elevation', 'slope']
//print(l8.bandNames().slice(1));
var classifier2000 = ee.Classifier.smileRandomForest(100).train({
  features: trainSet,
  classProperty: 'Class',
  inputProperties: trainingBands
});

var classified_2000 = l7_2000.classify(classifier2000);
//Add final map to the display
Map.addLayer(classified_2000, {min:0, max:3, palette:['red','blue', 'green', 'yellow']} , "Land Classification 2000");

//Determine the Land Cover Change between 2001 and 2019
var LULC_Change = classified.add(classified_2000);
Map.addLayer(LULC_Change.clip(roi), {min:0, max:3, palette:['red','blue', 'green', 'yellow']}, 'LULC Change Between 2000 and 2020');
//-------------- CREATE A LEGEND ------------------

//set position of panel
var legend = ui.Panel({
  style: {
    position: 'bottom-left',
    padding: '8px 15px'
  }
});

//Create a legend title
var legendTitle = ui.Label({
  value: 'Classification Legend',
  style: {
    fontWeight: 'bold',
    fontSize: '18px',
    margin: '0 0 4px 0',
    padding: '0'
  }
});

//Add the title to the panel
legend.add(legendTitle);

//Create and style 1 row of the legend.

var makeRow = function(color, name){
  var colorBox = ui.Label({
    style: {
      backgroundColor: '#' + color,
      padding: '8px',
      margin: '0 0 4px 0'
    }
  });
  
  var description = ui.Label({
    value: name,
    style: {margin: '0 0 4px 6px'}
  });
  
  return ui.Panel({
    widgets: [colorBox, description],
    layout: ui.Panel.Layout.Flow('horizontal')
  });
  
};

//Identify palette with the legend colors
var palette =['FF0000','0000FF', '008000', 'FFFF00'];
 
//Identify names within the legend
var names = ['urban','water','vegetation', 'agricolture'];
 
//Add color and names
for (var i = 0; i < 4; i++) {
  legend.add(makeRow(palette[i], names[i]));
}  

//Add legend to map
Map.add(legend);

////******Part 6: Display the Final Land Cover Classification and Provide Export Options******
//////////////////////////////////////////////////////////////////////////////////////////////


//Add final map to the display
Map.addLayer(classified, {min:0, max:3, palette:['red','blue', 'green', 'yellow']} , "Land Classification");

//Map.addLayer(classified, {min:0, max:3, palette:['red','blue', 'green', 'yellow']},'LULC2023');
 
//---------------------- LST calculation ---------------------------------

var ndvi = l8.normalizedDifference(['SR_B5', 'SR_B4']).rename('NDVI');

//select thermal band 10 (with brightness temperature)
var thermal = l8.select('ST_B10').rename('thermal');
//var b10Params = {min: 291.918, max: 302.382, palette: ['blue', 'white', 'green']};
print(thermal);
//Map.addLayer(thermal, b10Params, 'thermal');

//find the min and max of NDVI
var minNDVI = ee.Number(ndvi.reduceRegion({
  reducer: ee.Reducer.min(),
  geometry: roi,
  scale: 30,
  maxPixels: 1e9
}).values().get(0));

print('Min NDVI:', minNDVI);

var maxNDVI = ee.Number(ndvi.reduceRegion({
  reducer: ee.Reducer.max(),
  geometry: roi,
  scale: 30,
  maxPixels: 1e9
}).values().get(0));

print('Max NDVI:', maxNDVI);

//Calculating fraction of vegetation
var fv = (ndvi.subtract(minNDVI).divide(maxNDVI.subtract(minNDVI))).pow(ee.Number(2)).rename('FV');
print('Fractional vegetation:', fv);
//Map.addLayer(fv, {}, 'fv');

//Calculating Emissivity
var a = ee.Number(0.004);
var b = ee.Number(0.986);
var em = fv.multiply(a).add(b).rename('EM');
//Map.addLayer(em, {}, 'EM');

// Now, lets calculate the land surface temperature (LST)

// Formula: (TB / (1 + ( * (TB / 1.438)) * ln(em))) - 273.15
var lst = thermal.expression(
  '(TB / (1 + (0.00115 * (TB / 0.48359547432)) * log(em))) - 273.15', {
    'TB': thermal.select('thermal'), // Select the thermal band (TB)
    'em': em // Assign emissivity (em)
  }).rename('LST');

// Add the LST Layer to the Map with Custom Visualization Parameters
Map.addLayer(lst, {
  min: 18.47, // Minimum LST value
  max: 42.86, // Maximum LST value
  palette: [
    '040274', '040281', '0502a3', '0502b8', '0502ce', '0502e6',
    '0602ff', '235cb1', '307ef3', '269db1', '30c8e2', '32d3ef',
    '3be285', '3ff38f', '86e26f', '3ae237', 'b5e22e', 'd6e21f',
    'fff705', 'ffd611', 'ffb613', 'ff8b13', 'ff6e08', 'ff500d',
    'ff0000', 'de0101', 'c21301', 'a71001', '911003'
  ]}, 'Land Surface Temperature');


var averageLST = lst.reduceRegion({
  reducer: ee.Reducer.mean(),
  geometry: roi,
  scale: 30}).values().getInfo()[0];

print('Average LST:',averageLST);

Map.centerObject(roi);
